
<div id="root"></div>

<script>

	var options = {
		redirect: false,
		popupOptions: { width: 300, height: 400, left: 200, top: 300 }
	}; 

    var lock = new Auth0Lock("{{env.AUTH0_CLIENT_ID}}", "{{env.AUTH0_DOMAIN}}", { auth: {
        redirectUrl: "{{env.AUTH0_CALLBACK_URL}}", 
        responseType: "code", 
        params: {
          	scope: "openid name email picture"
    	}
    }}, options);
    
    lock.show();

</script>







